# Generated by Chef run for <%= node['fqdn'] %>

Host *
  Protocol 2
  ConnectTimeout 10

  <% if @keep_alive %>
    TCPKeepAlive yes
    ServerAliveInterval <%= @timeout %>
  <% end %>

  Compression <%= @settings['compression'] ? 'yes' : 'no' %>
  ForwardAgent <%= @settings['forward_agent'] ? 'yes' : 'no' %>
  PreferredAuthentications <%= Array(@settings['authentication']).join(",") %>

